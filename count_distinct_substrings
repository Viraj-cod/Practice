def substring(st):
    res = ['']
    for i in range(len(st)):
        for j in range(i+1,len(st)+1):
            res.append(st[i:j])
    return res
    
def counDistictSubstring():
    st1 = 'ab'
    st2 = 'ababa'
    res1 = substring(st1)
    res2 = substring(st2)
    print(len(set(res1)))
    print(len(set(res2)))

counDistictSubstring()
    
