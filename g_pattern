n=10
for i in range(1,n):
    for j in range(1,n):
        if i ==n-4 and j <n-1:
            print('*',end= ' ')
        elif j >= n-1 and i > n-7:
            print('*',end=' ')
        elif i==1 or (j ==1 and i <n-4) :
            print('*',end=' ')
        else:
            print(' ',end=' ')
    print()
