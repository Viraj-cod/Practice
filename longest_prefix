#arr = ['geeksforgeeks','geeks','geek','geezer']
arr = ['kamini','kamiyani','kalika']
ln = [len(i) for i in arr]
res = len(arr)
final = []
for i in range(res):
    count = 0
    k = 0
    for j in range(res):
        if arr[k][i] == arr[j][i]:
            count = count + 1
    if count == res:
        final.append(arr[k][i])
print(*final)

    
                
            
